<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Menu"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="320dp"
        android:orientation="vertical"
        android:background="@color/orange">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:orientation="horizontal"
            android:layout_marginTop="30dp"
            android:layout_marginLeft="20dp">

            <ImageButton
                android:id="@+id/imageButton"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"

                app:srcCompat="@drawable/ic_back"
                android:background="@color/orange"
                android:layout_marginTop="8dp"
                android:layout_marginLeft="10dp"/>

            <TextView
                android:id="@+id/textView"
                android:layout_width="wrap_content"
                android:layout_height="42dp"
                android:gravity="center"
                android:text="My Food List"
                android:textColor="@color/white"
                android:layout_marginLeft="20dp"
                android:textSize="20dp"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <ImageButton
                android:id="@+id/imageButton12"
                android:layout_width="216dp"
                android:layout_height="170dp"
                android:layout_marginLeft="100dp"
                app:srcCompat="@drawable/ic_person" />

            <TextView
                android:id="@+id/textView20"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:gravity="center"
                android:text="Test Restaurant"
                android:textColor="@color/white"
                android:textFontWeight="500"
                android:textSize="20dp"/>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="406dp"
        android:orientation="vertical"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="47dp"
            android:orientation="horizontal">

            <ImageButton
                android:id="@+id/imageButton13"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/ic_person"
                android:background="@color/white"
                android:layout_marginTop="10dp"

                />

            <TextView
                android:id="@+id/textView21"
                android:layout_width="270dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="Person Info"
                android:gravity=""
                android:layout_marginLeft="10dp"/>

            <ImageButton
                android:id="@+id/imageButton16"
                android:layout_width="62dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                app:srcCompat="@drawable/ic_navigate_next_24"
                android:background="@color/white"
                android:layout_marginTop="10dp"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="51dp"
            android:orientation="horizontal">
            <ImageButton
                android:id="@+id/imageButton13"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/ic_settings_24"
                android:background="@color/white"
                android:layout_marginTop="10dp"/>

            <TextView
                android:id="@+id/textView21"
                android:layout_width="270dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="Settings"
                android:gravity=""
                android:layout_marginLeft="10dp"/>

            <ImageButton
                android:id="@+id/imageButton16"
                android:layout_width="62dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                app:srcCompat="@drawable/ic_navigate_next_24"
                android:background="@color/white"
                android:layout_marginTop="10dp"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="51dp"
            android:orientation="horizontal">
            <ImageButton
                android:id="@+id/imageButton13"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/ic_revenue_24"
                android:background="@color/white"
                android:layout_marginTop="10dp"/>

            <TextView
                android:id="@+id/textView21"
                android:layout_width="270dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="Revenue"
                android:gravity=""
                android:layout_marginLeft="10dp"/>

            <ImageButton
                android:id="@+id/imageButton16"
                android:layout_width="62dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                app:srcCompat="@drawable/ic_navigate_next_24"
                android:background="@color/white"
                android:layout_marginTop="10dp"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="46dp"
            android:orientation="horizontal">

            <ImageButton
                android:id="@+id/imageButton13"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/ic_quantity_24"
                android:background="@color/white"
                android:layout_marginTop="10dp"/>

            <TextView
                android:id="@+id/textView21"
                android:layout_width="270dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="Number Of Oders"
                android:gravity=""
                android:layout_marginLeft="10dp"/>

            <TextView
                android:id="@+id/textView22"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="29K" />


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="52dp"
            android:orientation="horizontal">
            <ImageButton
                android:id="@+id/imageButton13"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/ic_keyboard_command_key_24"
                android:background="@color/white"
                android:layout_marginTop="10dp"/>

            <TextView
                android:id="@+id/textView21"
                android:layout_width="270dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:text="User Reviews"
                android:gravity=""
                android:layout_marginLeft="10dp"/>

            <ImageButton
                android:id="@+id/imageButton16"
                android:layout_width="62dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                app:srcCompat="@drawable/ic_navigate_next_24"
                android:background="@color/white"
                android:layout_marginTop="10dp"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:orientation="horizontal">
            <ImageButton
                android:id="@+id/imageButton13"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/ic_logout_24"
                android:background="@color/white"
                android:layout_marginTop="10dp"/>

            <TextView
                android:id="@+id/textView21"
                android:layout_width="270dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="10dp"
                android:layout_weight="1"
                android:gravity=""
                android:text="Log Out" />

            <ImageButton
                android:id="@+id/imageButton16"
                android:layout_width="62dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                app:srcCompat="@drawable/ic_navigate_next_24"
                android:background="@color/white"
                android:layout_marginTop="10dp"/>
        </LinearLayout>
    </LinearLayout>

</LinearLayout>