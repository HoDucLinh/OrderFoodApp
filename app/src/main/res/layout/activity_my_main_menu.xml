<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/Light"
    tools:context=".view.MyMainMenu">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="@dimen/padding_layout"
        android:gravity="center"
        android:orientation="horizontal">
        <ImageButton
            android:id="@+id/backHome"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:src="@drawable/back" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:fontFamily="@font/sen"
            android:text="Profile"
            android:layout_weight="1"
            android:textSize="20dp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:orientation="horizontal">

        <ImageView
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:layout_marginLeft="30dp"
            android:background="@drawable/round_background"
            android:clipToOutline="true"
            android:src="@drawable/personal" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="20dp"
            android:orientation="vertical">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/sen_bold"
                android:text="Vishal Khadok"
                android:textSize="20dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:fontFamily="@font/sen"
                android:text="I love fast food"
                android:textSize="16dp" />

        </LinearLayout>

    </LinearLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:background="@drawable/rounded_linear_layout"
            android:orientation="horizontal"
            android:paddingTop="10dp"
            android:paddingBottom="15dp"
            android:paddingLeft="5dp"
            android:paddingRight="5dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/personalInfo"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:backgroundTint="@color/WhisperWhite"
                    android:drawableLeft="@drawable/icon_user"
                    android:gravity="start|center"
                    android:text="Personal Info"
                    android:drawableRight="@drawable/ic_navigate_next_24"
                    android:textAllCaps="false"
                    android:layout_marginLeft="10dp"
                    android:fontFamily="@font/sen_bold"
                    android:textSize="16dp"
                    android:textColor="@color/textDark"
                    />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:backgroundTint="@color/WhisperWhite"
                    android:gravity="start|center"
                    android:drawableLeft="@drawable/icon_address"
                    android:text="Address"
                    android:textAllCaps="false"
                    android:layout_marginLeft="10dp"
                    android:fontFamily="@font/sen_bold"
                    android:textSize="16dp"
                    android:drawableRight="@drawable/ic_navigate_next_24"
                    android:textColor="@color/textDark" />

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:background="@drawable/rounded_linear_layout"
            android:orientation="horizontal"
            android:paddingTop="10dp"
            android:paddingBottom="15dp"
            android:paddingLeft="5dp"
            android:paddingRight="5dp">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:backgroundTint="@color/WhisperWhite"
                    android:gravity="start|center"
                    android:text="Cart"
                    android:drawableLeft="@drawable/icon_cart"
                    android:textAllCaps="false"
                    android:layout_marginLeft="10dp"
                    android:fontFamily="@font/sen_bold"
                    android:drawableRight="@drawable/ic_navigate_next_24"
                    android:textSize="16dp"
                    android:textColor="@color/textDark" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:backgroundTint="@color/WhisperWhite"
                    android:gravity="start|center"
                    android:drawableLeft="@drawable/icon_heart"
                    android:text="Favorite"
                    android:textAllCaps="false"
                    android:layout_marginLeft="10dp"
                    android:drawableRight="@drawable/ic_navigate_next_24"
                    android:fontFamily="@font/sen_bold"
                    android:textSize="16dp"
                    android:textColor="@color/textDark" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:backgroundTint="@color/WhisperWhite"
                    android:gravity="start|center"
                    android:text="Notification"
                    android:drawableLeft="@drawable/icon_notification"
                    android:textAllCaps="false"
                    android:layout_marginLeft="10dp"
                    android:fontFamily="@font/sen_bold"
                    android:drawableRight="@drawable/ic_navigate_next_24"
                    android:textSize="16dp"
                    android:textColor="@color/textDark" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:backgroundTint="@color/WhisperWhite"
                    android:gravity="start|center"
                    android:text="Payment Method"
                    android:drawableLeft="@drawable/icon_payment"
                    android:textAllCaps="false"
                    android:layout_marginLeft="10dp"
                    android:fontFamily="@font/sen_bold"
                    android:drawableRight="@drawable/ic_navigate_next_24"
                    android:textSize="16dp"
                    android:textColor="@color/textDark" />


            </LinearLayout>


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:background="@drawable/rounded_linear_layout"
            android:orientation="vertical"
            android:paddingTop="10dp"
            android:paddingBottom="15dp"
            android:paddingLeft="5dp"
            android:paddingRight="5dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:backgroundTint="@color/WhisperWhite"
                        android:fontFamily="@font/sen_bold"
                        android:text="FAQs"
                        android:gravity="start|center"
                        android:drawableLeft="@drawable/icon_help"
                        android:textAllCaps="false"
                        android:drawableRight="@drawable/ic_navigate_next_24"
                        android:textColor="@color/textDark"
                        android:textSize="16dp" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:backgroundTint="@color/WhisperWhite"
                        android:drawableLeft="@drawable/icon_review"
                        android:fontFamily="@font/sen_bold"
                        android:gravity="start|center"
                        android:text="User Reviews"
                        android:textAllCaps="false"
                        android:drawableRight="@drawable/ic_navigate_next_24"
                        android:textColor="@color/textDark"
                        android:textSize="16dp" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:backgroundTint="@color/WhisperWhite"
                        android:fontFamily="@font/sen_bold"
                        android:gravity="start|center"
                        android:drawableLeft="@drawable/icon_setting"
                        android:text="Settings"
                        android:textAllCaps="false"
                        android:drawableRight="@drawable/ic_navigate_next_24"
                        android:textColor="@color/textDark"
                        android:textSize="16dp" />

                </LinearLayout>

            </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:background="@drawable/rounded_linear_layout"
            android:orientation="horizontal"
            android:paddingTop="10dp"
            android:paddingBottom="15dp"
            android:paddingLeft="5dp"
            android:paddingRight="5dp">


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:backgroundTint="@color/WhisperWhite"
                    android:gravity="start|center"
                    android:text="Log Out"
                    android:textAllCaps="false"
                    android:layout_marginLeft="10dp"
                    android:drawableLeft="@drawable/icon_logout"
                    android:fontFamily="@font/sen_bold"
                    android:drawableRight="@drawable/ic_navigate_next_24"
                    android:textSize="16dp"
                    android:textColor="@color/textDark" />


            </LinearLayout>

        </LinearLayout>

    </LinearLayout>


</LinearLayout>
</LinearLayout>